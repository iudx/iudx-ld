{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "@context": [
        "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_context.json",
        "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/common_context.json",
        "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/miscSchemaOrgDefs.json",
        "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/geometry-schema.json",
        "https://raw.githubusercontent.com/iudx/iudx-ld/master/data_models/gis/gis_context.json"
    ],
    "describes": "Ward Boundary",
    "properties": {
        "CLASS": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "Class Code and Sub Class Code will be"
        },
        "GCP_HT": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "As per DGPS value in meter"
        },
        "GCP_LAT": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/GeoProperty",
            "describes": "Geographical coordinates (Latitude) as per DGPS for the location in decimal degree like  25\u00b019'40.52\"N"
        },
        "GCP_LON": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/GeoProperty",
            "describes": "Geographical coordinates (longitude) as per DGPS for the location in Degree Minutes  Seconds like 83\u00b0 0'31.03\"E"
        },
        "GCP_PHID": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "Unique ID for Photo will start with GCP ID followed by 4 digits of Year (like 2018) then  month (like for June it will be 06) then date  (01) and then 2 digits in incremental order  (like 01 to 99)"
        },
        "SUB_CLASS": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "updated as per AMRUT guidelines given in Annexure XI, Volume II of the Inception  Report"
        },
        "SURROUND": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "Brief description about surroundings in all the directions East, West, North and South"
        },
        "TIME_ST": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/TimeProperty",
            "describes": "Date of DGPS survey (DD-Month-YYYY) like 30 June 2018"
        },
        "W_AREA": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Ward area for each ward in sq. km as per GIS- based calculation"
        },
        "W_HH": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Total number of households as per census data"
        },
        "W_HH_SC": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Total number of household having Sewer connection  based  on  consumer  database  survey; will be updated after survey"
        },
        "W_HH_WC": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Total number of household having water connection based on consumer survey; will be  updated after survey"
        },
        "W_ID": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "It will start with W followed by 001 for ward 1, 002 for ward 2 and so on"
        },
        "W_NAME": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "Like Naria, Cantonmenent etc."
        },
        "W_NUM": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "Ward Number like 001, 002"
        },
        "W_POP": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Population of Ward as per 2011 census data"
        },
        "W_PROP": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Total number of properties based on property survey; will be updated after survey"
        },
        "W_SL_AR": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/QuantitativeProperty",
            "describes": "Total area of slum based on slum survey for GIS base map preparation (in Ha)"
        },
        "Z_ID": {
            "$ref": "https://raw.githubusercontent.com/iudx/iudx-ld/master/base_schemas/v0.0.0/core_defs.json#/definitions/Property",
            "describes": "As per zone table"
        }
    },
    "type": "object"
}
